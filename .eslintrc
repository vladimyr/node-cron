{
	"extends": ["standard", "prettier", "prettier/standard"],
	"parserOptions": {
		"ecmaVersion": 2017,
		"sourceType": "module"
	},
	"env": {
		"browser": true,
		"node": true,
		"jest/globals": true
	},
	"plugins": ["prettier", "standard", "jest"],
	"rules": {
		"space-before-function-paren": "off",
		"new-cap": "off",
		"prettier/prettier": "error"
	}
}
